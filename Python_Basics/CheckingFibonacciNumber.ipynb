#Program to check whether a given number is Fibonacci number or not

def fibo(n):
    if (n==0): return 0
    elif (n==1):return 1
    else:
        val = fibo (n-1) + fibo (n-2)
        return val
    
num = int(input('Number to check  '))

for i in range(1,num+1):
    if fibo(i)!=num:
        continue
    elif(fibo(i)==num): 
        print('Number is Fibonacci Number') 
        break
else: print('Number is not Fibonacci Number')