def quick_sort(arr,low,high) :
    pivote=arr[high - 1]
    for i in range(low,high):
        print('i=',i)
        if(arr[i]<pivote):
            temp = arr[i]
            arr[i]=arr[i-1]
            arr[i-1]=temp
            continue
        if(arr[i]>pivote):
            key = i-1
            continue
        if(arr[i]==pivote):
            print('key=',key)
            temp = arr[key]
            arr[key] = arr[high - 1]
            arr[high - 1]=temp
    return arr

if(key>1):
    low=0
    high=key
    quick_sort(arr,low,high)
    low = key
    high=n
    quick_sort(arr,low,high)

